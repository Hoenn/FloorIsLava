        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Physics Step
//Local vars for current keys
var rkey= keyboard_check(vk_right);
var lkey = keyboard_check(vk_left);
var jkey = keyboard_check(vk_space);

//Check if offscreen
if(x &gt; room_width || x &lt; 0 || y &gt; room_height || y &lt; 0)
{
    room_restart();
}
//Check ground
if(place_meeting(x, y+1, obj_wall)) 
{
    vspd=0;
    if(jkey)
    {
        start=true;
        vspd = - jspd;
    }
}
else
{
    if(vspd &lt; terminal)
        vspd+=grav;
        
    
}

//Move Right
if(rkey)
{
    if(hspd &lt; spd)
        hspd+=fric;
    else
        hspd = spd;
    
    if(place_meeting(x-1, y, obj_wall) &amp;&amp; !place_meeting(x, y+1, obj_wall) &amp;&amp; !lkey)
    {
        vspd = -jspd;
    }
}

//Move Left
if(lkey)
{
    if(hspd &gt; -spd)
        hspd -= fric;
    else
        hspd = -spd;
    
    if(place_meeting(x+1, y, obj_wall) &amp;&amp; !place_meeting(x, y+1, obj_wall) &amp;&amp; !rkey)
    {
        vspd = -jspd   
    }
}

//Both or no movement keys
if((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey))
{
    if(hspd != 0)
    {
        if(hspd &lt; 0)
            hspd += fric;
        else
            hspd -= fric;
    }
}

    
//Horizontal Collision
if(place_meeting(x+hspd, y, obj_wall))
{
    while(!place_meeting(x+sign(hspd), y, obj_wall))
    {
        x+=sign(hspd);
    }
    hspd=0;
}

x += hspd;

//Vertical Collisions
if(place_meeting(x, y+vspd, obj_wall))
{
    while(!place_meeting(x, y+sign(vspd), obj_wall))
    {
        y+=sign(vspd);
    }
    vspd=0;
}

y += vspd;


if(!hasBoots &amp;&amp; place_meeting(x, y, the_boots))
{
    hasBoots = true;
    my_boots = instance_create(x-x_offset, y-y_offset, obj_boots);
    with(the_boots)
        instance_destroy();
}
else if (!hasBoots)
{
    t+=increment;
    shift = amplitude * sin(t);
    yy +=the_boots.vspeed;
    the_boots.y = yy + shift;
}
if(hasBoots)
{
    if(lkey) 
        my_boots.image_xscale= -1;
    else if(rkey)
        my_boots.image_xscale = 1;
    my_boots.x = x-x_offset;
    my_boots.y = y-y_offset;
    jspd = 8;
}


</string>
          </argument>
        </arguments>
      </action>
